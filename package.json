{
  "name": "Bitcat",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.1",
    "@react-spring/web": "^9.4.4",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "dayjs": "^1.11.0",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-root-path": "^1.0.16",
    "electron-store": "^8.0.1",
    "html2canvas": "^1.4.1",
    "html2canvas-render-offscreen": "^0.0.3",
    "immer": "^9.0.12",
    "jotai": "^1.6.1",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.4",
    "react-flow-renderer": "^9.7.4",
    "react-responsive-spritesheet": "^2.3.9",
    "react-rnd": "^10.3.5",
    "react-router-dom": "6",
    "react-scripts": "5.0.0",
    "react-spritesheet": "^1.0.1",
    "react-textarea-autosize": "^8.3.3",
    "react-use-measure": "^2.1.1",
    "typescript": "^4.6.2",
    "use-react-screenshot": "^3.0.0",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.4"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "productName": "Bitcat",
    "asar": true,
    "appId": "com.enfpdev.bitcat",
    "mac": {
      "target": "dmg",
      "hardenedRuntime": true,
      "gatekeeperAssess": true,
      "extendInfo": {
        "NSScreenCaptureDescription": "Let me take screenshots"
      }
    }
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "start": "concurrently \"cross-env NODE_ENV=development BROWSER=none craco start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "craco build && electron-builder",
    "release": "craco build && electron-builder --publish=always"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "@types/react-color": "^3.0.6",
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron": "^17.1.0",
    "electron-builder": "^22.14.13",
    "wait-on": "^6.0.1"
  }
}
